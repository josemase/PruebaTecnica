<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://parameta.com/test" targetNamespace="http://parameta.com/test" elementFormDefault="qualified">

    <xs:element name="addEmpleadoRequest" type="tns:addEmpleadoRequest"/>
    <xs:element name="addEmpleadoResponse" type="tns:addEmpleadoResponse"/>

    <xs:complexType name="addEmpleadoRequest">
        <xs:sequence>
            <xs:element name="name" type="xs:string"/>
            <xs:element name="lastname" type="xs:string"/>
            <xs:element name="role" type="xs:string"/>
            <xs:element name="salary" type="xs:double"/>
            <xs:element name="startDate" type="xs:date"/>
            <xs:element name="bornDate" type="xs:date"/>
            <xs:element name="documentType" type="xs:string"/>
            <xs:element name="documentNumber" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="addEmpleadoResponse">
        <xs:sequence>
            <xs:element name="message" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>